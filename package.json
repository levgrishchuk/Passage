  },
  "homepage": "https://github.com/moonwane/node-testing#readme",
  "dependencies": {
    "bootstrap": "^5.1.1",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "mongoose": "^6.0.7",
    "nodemon": "^2.0.7",
    "spotify-web-api-node": "^5.0.2"
  },
  "engines": {
    "node": "12.19.0"
  }
